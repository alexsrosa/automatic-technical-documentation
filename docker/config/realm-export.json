{
  "id": "DocGenRealm",
  "realm": "DocGenRealm",
  "enabled": true,
  "sslRequired": "none",
  "clients": [
    {
      "clientId": "docgen-web",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "http://localhost:3000"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "User role"
      },
      {
        "name": "admin",
        "description": "Admin role"
      }
    ]
  }
}
