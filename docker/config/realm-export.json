{
  "id": "docgen",
  "realm": "docgen",
  "enabled": true,
  "sslRequired": "none",
  "loginTheme": "docgen-theme",
  "users": [
    {
      "username": "user",
      "enabled": true,
      "email": "user@example.com",
      "firstName": "Test",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ]
    }
  ],
  "clients": [
    {
      "clientId": "docgen-frontend",
      "enabled": true,
      "publicClient": false,
      "secret": "change-me",
      "redirectUris": [
        "http://localhost:4000/*"
      ],
      "webOrigins": [
        "http://localhost:4000"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "User role"
      },
      {
        "name": "admin",
        "description": "Admin role"
      }
    ]
  }
}
